///                  ||  \||  /    \  ||../   ||  ||    ||
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t,k=1;cin>>t;
    while(t--){
        int n,s=0;cin>>n;
        for(int i=0;i<n;i++){
            int a;cin>>a;
            if(a>=0)s+=a;
        }
        cout<<"Case "<<k++<<": "<<s<<endl;
    }
    return 0;
}
///...........Alhamdulillah.........///

